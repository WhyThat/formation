{
  "parser": "babel-eslint",
  "extends": ["airbnb", "plugin:css-modules/recommended", "prettier"],
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "import/prefer-default-export": "off",
    "max-len": ["error", 120],
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "react/prop-types": 1,
    "jsx-a11y/accessible-emoji": 0
  },
  "plugins": [
    "css-modules"
  ]
}
